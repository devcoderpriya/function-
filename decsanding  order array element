int main() {
    int a;
    printf("enter the element:-");
    scanf("%d",&a);
    int arr[a];
    int temp;
    int i,j;
    for(i=0;i<a;i++){
    scanf("%d",&arr[i]);
}
    for(i=0;i<a;i++){
        for(j=i+1;j<a;j++){
            if(arr[i]<arr[j]){
                temp=arr[i];
                arr[j]=arr[i];
                temp=arr[j];
            }
        }
    }
    printf("enter the array element:-");
      for(i=0;i<a;i++){
          printf("%d",arr[i]);
      }
    return 0;
    
}
