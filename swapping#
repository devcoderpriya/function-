#include<stdio.h>
{
void swap(int a,int b)
a=a+b
b=a-b
a=a+b
printf("value of a and b after swappinf is a=%d and b=%d",a,b);
}
int main()
{
int x;
printf("enter the value of x:-");
scanf("%d",&x);

int y;
printf("enter the value of x:-");
scanf("%d",&y);

printf("value of a and b befofe swapping is x=%d and y=%d",x,y);

swap(x,y)

return 0;
}

