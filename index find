#include <stdio.h>

int main() {
    int a; 
    printf("Enter the size of the array: ");
    scanf("%d", &a);
      
    int arr[a];

    for (int i = 0; i < a; i++) {
       scanf("%d", &arr[i]);
    }
    int index;
    printf("Enter the index number: ");
    scanf("%d", &index);
    if (index >= 0 && index < a) {
        printf("The value at index %d is: %d\n", index, arr[index]);
    } else {
        printf("Error: Index out of range.\n");
    }

    return 0;
}
